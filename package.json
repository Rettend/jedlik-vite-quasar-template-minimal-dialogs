{
  "name": "jedlik-vite-quasar-minimal",
  "version": "1.0.0",
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "scripts": {
    "dev": "vite --port 8080 --open",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --ignore-path .gitignore --fix",
    "format": "prettier --write ./**/*.{vue,ts,tsx,js,jsx,css,less,scss,json,md}",
    "test": "vitest --run --coverage --reporter verbose",
    "test:coverage": "vitest run --coverage",
    "cy:open": "npx cypress open --browser=edge",
    "cy:run": "npx cypress run"
  },
  "dependencies": {
    "@quasar/extras": "^1.15.8",
    "@vueuse/core": "^9.6.0",
    "@vueuse/math": "^9.6.0",
    "@vueuse/shared": "^9.6.0",
    "axios": "^1.2.1",
    "pinia": "^2.0.28",
    "quasar": "^2.10.2",
    "tslib": "^2.4.1",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vue3-google-login": "^2.0.12"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@pinia/testing": "^0.0.14",
    "@quasar/vite-plugin": "^1.2.3",
    "@types/node": "^18.11.15",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitest/coverage-istanbul": "^0.25.8",
    "@vue/test-utils": "^2.2.6",
    "cypress": "^12.1.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "happy-dom": "^8.1.0",
    "sass": "1.32.12",
    "typescript": "^4.9.4",
    "vite": "^3.2.5",
    "vitest": "^0.25.8",
    "vue-tsc": "^1.0.13"
  }
}

{
  "name": "jedlik-vite-quasar-minimal",
  "version": "1.0.0",
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "scripts": {
    "dev": "vite --port 8080 --open",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --ignore-path .gitignore --fix",
    "format": "prettier --write ./**/*.{vue,ts,tsx,js,jsx,css,less,scss,json,md}",
    "test": "vitest --run --coverage --reporter verbose",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cypress open"
  },
  "dependencies": {
    "@quasar/extras": "^1.15.5",
    "@vueuse/core": "^9.3.0",
    "@vueuse/math": "^9.3.0",
    "@vueuse/shared": "^9.3.0",
    "axios": "^1.1.2",
    "pinia": "^2.0.23",
    "quasar": "^2.10.0",
    "tslib": "^2.4.0",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5",
    "vue3-google-login": "^2.0.12"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.14",
    "@quasar/vite-plugin": "^1.2.3",
    "@types/node": "^16.11.60",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitest/coverage-istanbul": "^0.24.3",
    "@vue/test-utils": "^2.1.0",
    "cypress": "^10.10.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.6.0",
    "happy-dom": "^7.5.12",
    "sass": "1.32.12",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3",
    "vue-tsc": "^1.0.7"
  }
}
